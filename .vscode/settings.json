{
  "python.defaultInterpreterPath": "${workspaceFolder}/module/env/Scripts/python.exe",
  "mypy-type-checker.path": [
    "${workspaceFolder}/module/env/Scripts/mypy.exe"
  ],
  "mypy-type-checker.cwd": "${workspaceFolder}/module/",
  "mypy-type-checker.interpreter": [
    "${workspaceFolder}/module/env/Scripts/python.exe"
  ],
  "mypy-type-checker.ignorePatterns": [
    "**/site-packages/**/*.py",
    ".vscode/*.py",
    "**/env/Scripts/*.py"
  ],
  "mypy-type-checker.reportingScope": "workspace",
  "mypy-type-checker.args": [
    "--exclude=\"**/env/Scripts/*.py\""
  ],
  "pylint.path": [
    "${workspaceFolder}/module/env/Scripts/pylint.exe"
  ],
  "pylint.interpreter": [
    "${workspaceFolder}/module/env/Scripts/python.exe"
  ],
  "pylint.cwd": "${workspaceFolder}/module/",
  "isort.path": [
    "${workspaceFolder}/module/env/Scripts/isort.exe"
  ],
  "isort.interpreter": [
    "${workspaceFolder}/module/env/Scripts/python.exe"
  ],
  "isort.cwd": "${workspaceFolder}/module/",
  "flake8.path": [
    "${workspaceFolder}/module/env/Scripts/flake8.exe"
  ],
  "flake8.interpreter": [
    "${workspaceFolder}/module/env/Scripts/python.exe"
  ],
  "flake8.cwd": "${workspaceFolder}/module/",
  "autopep8.path": [
    "${workspaceFolder}/module/env/Scripts/autopep8.exe"
  ],
  "autopep8.interpreter": [
    "${workspaceFolder}/module/env/Scripts/python.exe"
  ],
  "autopep8.importStrategy": "fromEnvironment",
  "black-formatter.path": [
    "${workspaceFolder}/module/env/Scripts/black.exe"
  ],
  "black-formatter.interpreter": [
    "${workspaceFolder}/module/env/Scripts/python.exe"
  ],
  "black-formatter.cwd": "${workspaceFolder}/module/",
  "python.analysis.stubPath": "${workspaceFolder}/module/env/Lib/site-packages",
  "python.analysis.indexing": true,
  "python.analysis.useLibraryCodeForTypes": true,
  "python.poetryPath": "${workspaceFolder}/module/env/Scripts/poetry.exe",
  "python.analysis.ignore": [
    "**/env/**/*.py",
    "**/env/**/*.pyi",
    "**/env/**/*.pyc"
  ],
  "python.analysis.typeCheckingMode": "standard",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportArgumentType": false
  }
}
